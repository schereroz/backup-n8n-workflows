    {
      "parameters": {
        "jsCode": "// 1. Pega os dados da empresa e da IA\nconst prepareData = $('Prepare Extract Data').first().json;\nconst aiResponse = $input.first().json;\n\n// 2. Extrai e limpa o texto JSON da resposta da IA\nlet responseText = aiResponse.choices?.[0]?.message?.content || '';\nresponseText = responseText.replace(/```json\\n?/g, '').replace(/```\\n?/g, '').trim();\n\n// 3. Faz o \"parse\" do texto para um objeto JSON\nlet parsedData;\ntry {\n  parsedData = JSON.parse(responseText);\n} catch (error) {\n  console.error('Erro ao fazer parse do JSON:', error);\n  return [];\n}\n\n// 4. Busca a lista de membros usando a chave correta\nconst members = parsedData.board_of_directors || [];\n\n// 5. Se n√£o encontrar membros, para a execu√ß√£o\nif (members.length === 0) {\n  console.log('‚ö†Ô∏è Nenhum membro encontrado!');\n  return [];\n}\n\n// 6. Formata a sa√≠da combinando os dados da empresa com os de cada membro\nreturn members.map(member => {\n  return {\n    json: {\n      // Dados da Empresa\n      company_number: prepareData.company_number,\n      company_name: prepareData.company_name,\n      url_website_source: prepareData.board_url,\n      \n      // Dados do Executivo\n      type: member.type || member.member_type || 'board_member', // Define valor padr√£o\n      full_name: member.full_name || member.name || null,\n      role: member.role || member.position || null\n    }\n  };\n});"
      },
