      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// Em modo \"Run Once for Each Item\", use $input diretamente\nconst company = $input.json;\n\n// Check if board data exists, otherwise use boardUrl from input\nconst boardUrl = company.board?.member?.link ? (company.board.member.link || []).join(' ').trim() : (company.boardUrl || '');\n\n// Log to debug the data structure\nconsole.log('üìä Input data for company:', company.company_name || company.name, JSON.stringify(company, null, 2));\n\n// Log missing board data\nif (!company.board && !company.boardUrl) {\n  console.log('‚ö†Ô∏è No board or boardUrl data for company:', company.company_name || company.name);\n}\n\nconsole.log('üîç Processando empresa:', company.company_name || company.name);\nconsole.log('üìã Board URL:', boardUrl);\n\n// SEMPRE retorna o objeto\nreturn {\n  json: {\n    company_number: company.company_number || company.number,\n    company_name: company.company_name || company.name,\n    board_url: boardUrl\n  }\n};"
      },
